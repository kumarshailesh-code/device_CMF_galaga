allow gps_dump conninfra_device:chr_file rw_file_perms;
allow gps_dump tmpfs:lnk_file r_file_perms;
allow gps_dump mnt_user_file:dir search;
allow gps_dump mnt_user_file:lnk_file r_file_perms;
allow gps_dump storage_file:lnk_file r_file_perms;
allow gps_dump stp_dump_data_file:dir create_dir_perms;
allow gps_dump stp_dump_data_file:file create_file_perms;
allow gps_dump connsyslog_data_vendor_file:dir create_dir_perms;
allow gps_dump connsyslog_data_vendor_file:file create_file_perms;
get_prop(gps_dump, vendor_mtk_coredump_prop)
