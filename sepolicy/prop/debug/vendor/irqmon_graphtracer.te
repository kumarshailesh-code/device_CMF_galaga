# irqmon_graphtracer service
type irqmon_graphtracer, domain;
type irqmon_graphtracer_exec, exec_type, file_type, vendor_file_type;

init_daemon_domain(irqmon_graphtracer)

allow irqmon_graphtracer debugfs_tracing:file rw_file_perms;

userdebug_or_eng(`
  allow irqmon_graphtracer debugfs_tracing_debug:file rw_file_perms;
')

allow irqmon_graphtracer irqmon_vendor_file:file rw_file_perms;
allow irqmon_graphtracer irqmon_vendor_file:dir r_dir_perms;
