hal_attribute_hwservice(hal_mtk_aee, mtk_hal_aee_hwservice)
hal_attribute_service(hal_mtk_aee, mtk_hal_aee_service)

binder_call(hal_mtk_aee_client, hal_mtk_aee_server)
